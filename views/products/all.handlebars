<h1>Todos os produtos</h1>
<div class="products">
    {{#each products}}
        <div class="product">
            <img src="{{this.image}}" alt="{{this.name}}.png">
            <h3>{{this.name}}</h3>
            <p>R${{this.price}}</p>
            <div class="action">
                <a href="/products/product/{{this._id}}">Detalhes</a>
                <a href="/products/edit/{{this._id}}">Editar</a>
            </div>
        </div>
    {{/each}}
</div>
